{
  "components/common/body": "<div class=\"body\">\n    {{#block \"hero\"}} {{/block}}\n\n    <!-- Bolioptics -->\n    <div class=\"google-translate-container\" id=\"google_translate_container_mobile\"></div>\n\n    <div class=\"container\">\n        {{#block \"page\"}} {{/block}}\n    </div>\n    {{> components/common/modal}}\n    {{> components/common/modal-fullscreen}}\n    \n    <link rel=\"stylesheet\" type=\"text/css\" href=\"https://widget.reviews.io/floating-widget/css/dist.css\">\n<script src=\"https://widget.reviews.io/rich-snippet-reviews-widgets/dist.js\" type=\"text/javascript\"></script>\n\n<script>\n    window.onload = function() {\n        richSnippetReviewsWidgets({\n            store: \"boli-optics\",\n            primaryClr: \"#f7c41e\",\n            widgetName: \"floating-widget\",\n            numReviews: 40,\n            floatPosition: \"right\",\n            contentMode: \"company\",\n            hideDates: true\n        });\n    };\n</script>\n\n</div>\n",
  "components/common/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content jam\" oncopy=\"return false\" oncut=\"return false\" onpaste=\"return false\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n",
  "components/common/modal-fullscreen": "<div id=\"modal\" class=\"modal modal--fullscreen\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content jam\" oncopy=\"return false\" oncut=\"return false\" onpaste=\"return false\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n"
}