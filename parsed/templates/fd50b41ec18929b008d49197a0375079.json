{
  "components/common/breadcrumbs": "<div class=\"breadcrumbs-container\">\n    <ul class=\"breadcrumbs\">\n        {{#each breadcrumbs}}\n            <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\">\n                {{#if url}}\n                    <a href=\"{{url}}\" class=\"breadcrumb-label\">{{name}}</a>\n                {{else}}\n                    <span class=\"breadcrumb-label\">{{name}}</span>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n    <div class=\"google-translate-container\" id=\"google_translate_container_desktop\">\n        <div id=\"google_translate_element\"></div>\n    </div>\n</div>\n"
}