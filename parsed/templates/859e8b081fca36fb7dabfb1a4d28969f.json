{
  "components/emthemes-modez/sections/section-products-by-category": "\n\n{{> components/emthemes-modez/category/ajax-products-by-category\n    category_id=(lang (concat \"emthemesmodez.products_by_category.category_id_\" index))\n    image=(lang (concat \"emthemesmodez.products_by_category.image_\" index))\n    heading=(lang (concat \"emthemesmodez.products_by_category.heading_\" index))\n    text=(lang (concat \"emthemesmodez.products_by_category.text_\" index))\n    button=(lang (concat \"emthemesmodez.products_by_category.button_\" index))\n    style=(lang (concat \"emthemesmodez.products_by_category.style_\" index))\n}}\n\n",
  "components/emthemes-modez/category/ajax-products-by-category": "{{#each categories}}\n    {{#if id '==' ../category_id}}\n        <section class=\"emthemesModez-productsByCategory {{#if ../../style}}{{#if ../../../style '!=' ' '}}emthemesModez-productsByCategory--{{../../../../style}}{{/if}}{{/if}}\">\n            <div class=\"emthemesModez-productsByCategory-imageColumn\">\n                <figure class=\"emthemesModez-productsByCategory-figure\">\n                    <img class=\"emthemesModez-productsByCategory-image\" src=\"{{../../image}}\" alt=\"{{../../heading}}\">\n                    <figcaption class=\"emthemesModez-productsByCategory-figcaption\">\n                        <div class=\"emthemesModez-productsByCategory-figcaption-body\">\n                            <h3 class=\"emthemesModez-productsByCategory-heading\">{{../../heading}}</h3>\n                            <div class=\"emthemesModez-productsByCategory-text\">{{../../text}}</div>\n                            <a class=\"emthemesModez-productsByCategory-button button\" href=\"{{url}}\">{{../../button}}</a>\n                        </div>\n                    </figcaption>\n                </figure>\n            </div>\n\n            <div class=\"emthemesModez-productsByCategory-productsColumn\" data-emthemesmodez-products-by-category=\"{{url}}\">\n                {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\n            </div>\n        </section>\n    {{/if}}\n{{/each}}\n",
  "components/emthemes-modez/common/product-loading": "<div class=\"emthemesModez-productLoading emthemesModez-productLoading--maxCol2\">\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n</div>\n"
}