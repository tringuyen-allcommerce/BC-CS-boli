{
  "components/carousel": "<section class=\"heroCarousel {{#if theme_settings.homepage_stretch_carousel_images}}heroCarousel--stretch{{/if}}\"\n    data-slick='{\n        \"dots\": true,\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"autoplaySpeed\": {{carousel.swap_frequency}}\n    }'>\n    {{#each carousel.slides}}\n    <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}heroCarousel-slide--stretch{{/if}} heroCarousel-slide--{{@index}}\"\n        style=\"background-image: url({{image}})\">\n        <a href=\"{{url}}\">\n            {{#if (get (concat 'carousel-mobile-image' @index) ../theme_settings)}}\n                <img class=\"heroCarousel-mobileImage\" src=\"{{get (concat 'carousel-mobile-image' @index) ../../theme_settings}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"/>\n            {{/if}}\n            <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"/>\n            {{#if heading}}\n                {{> components/carousel-content show_background=true}}\n            {{else}}\n                {{#if text}}\n                    {{> components/carousel-content show_background=true}}\n                {{else}}\n                    {{#if button_text}}\n                        {{> components/carousel-content show_background=false}}\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        </a>\n    </div>\n    {{/each}}\n</section>\n",
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h3 class=\"heroCarousel-title\">{{heading}}</h3>\n    <p class=\"heroCarousel-description\">{{text}}</p>\n    {{#if button_text}}\n        <span class=\"heroCarousel-action button button--primary button--large\">{{button_text}}</span>\n    {{/if}}\n</div>\n"
}