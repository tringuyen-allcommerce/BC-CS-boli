{
  "components/emthemes-modez/instagram/carousel": "<div class=\"emthemesModez-instafeed-container {{#if style}}emthemesModez-instafeed-container--{{style}}{{/if}}\">\n    {{#unless hide_heading}}\n    <h2 class=\"page-heading\">{{lang 'instagram.title'}}</h2>\n    {{/unless}}\n    {{#if show_subheading}}\n    <div class=\"emthemesModez-instafeed-desc\">{{{lang 'instagram.description'}}}</div>\n    {{/if}}\n    <div id=\"instafeed\" class=\"emthemesModez-instafeed emthemesModez-instafeed--carousel {{#if style}}emthemesModez-instafeed--{{style}}{{/if}}\" \n        data-emthemesmodez-instafeed-carousel='{\n            \"dots\": false,\n            \"infinite\": false,\n            \"mobileFirst\": true,\n            \"slidesToShow\": 2,\n            \"slidesToScroll\": 2,\n            \"responsive\": [\n                {\n                    \"breakpoint\": 1260,\n                    \"settings\": {\n                        \"slidesToScroll\": 2,\n                        \"slidesToShow\": {{#if columns}}{{columns}}{{else}}6{{/if}}\n                    }\n                },\n                {\n                    \"breakpoint\": 800,\n                    \"settings\": {\n                        \"slidesToScroll\": 2,\n                        \"slidesToShow\": {{#if columns}}{{columns}}{{else}}6{{/if}}\n                    }\n                },\n                {\n                    \"breakpoint\": 550,\n                    \"settings\": {\n                        \"slidesToScroll\": 2,\n                        \"slidesToShow\": 2\n                    }\n                }\n            ]}'\n    ></div>\n</div>\n\n<script type=\"text/javascript\" src=\"{{cdn '/assets/vendor/instafeed/instafeed.min.js'}}\"></script>\n<script type=\"text/javascript\">\n// <![[\n    document.addEventListener('DOMContentLoaded', function () {\n        \"use strict\";\n        var feed = new Instafeed({\n            target: 'instafeed',\n            get: 'user',\n            userId: '{{lang \"instagram.user_id\"}}',\n            clientId: '{{lang \"instagram.client_id\"}}',\n            accessToken: '{{lang \"instagram.access_token\"}}',\n            resolution: 'standard_resolution',\n            template: '<div class=\"emthemesModez-instafeed-item\"><a href=\"{'+'{link}}\" target=\"_blank\" id=\"{'+'{id}}\"><img src=\"{'+'{image}}\" /></a></div>',\n            sortBy: 'most-recent',\n            limit: 12,\n            links: false,\n            after: function() {\n                document.getElementById(this.options.target).dispatchEvent(new CustomEvent(\"instafeedAfter\"));\n            }\n        });\n        feed.run();\n    });\n// ]]>\n</script>\n"
}