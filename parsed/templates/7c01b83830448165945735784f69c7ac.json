{
  "components/products/options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small class=\"emthemesModez-requiredMark\">{{lang 'common.required'}}</small>\n        {{/if}}\n\n        {{> components/emthemes-modez/products/field-guide}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n",
  "components/emthemes-modez/products/field-guide": "{{#each product_custom_fields}}\n    {{#startsWith (concat (concat \"__\" ../display_name) \"__\") name}}\n        <span class=\"emthemesModez-customField-guide\"><a href=\"{{value}}\" data-emthemesmodez-modal-content>{{#withAfter (split name \"__\") 2}}{{this}}{{/withAfter}}</a></span>\n    {{/startsWith}}\n{{/each}}\n"
}