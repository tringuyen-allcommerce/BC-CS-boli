{
  "components/papa-parallaxbag/sections/section-popular-categories": "<div class=\"emthemesModez-section emthemesModez-section--parallaxbag-popularCategories\">\n\t<div class=\"parallaxbag-popularCategoriesSection\">\n\t\t<h2 class=\"page-heading\">{{lang 'parallaxbag.popular_categories.heading'}}</h2>\n\t\t<div class=\"parallaxbag-popularCategoriesSection-columns\">\n\t\t\t{{#each (limit categories 3)}}\n\t\t\t<div class=\"parallaxbag-popularCategoriesSection-column\">\n\t\t\t\t{{#if image}}\n\t\t\t\t\t<div class=\"parallaxbag-popularCategoriesSection-image\"><a href=\"{{url}}\"><img src=\"{{getImage image 'zoom_size'}}\" alt=\"{{name}}\" /></a></div>\n\t\t\t\t{{/if}}\n\t\t\t\t<h3 class=\"parallaxbag-popularCategoriesSection-categoryName\"><a href=\"{{url}}\">{{name}}</a></h3>\n\t\t\t\t{{#if children}}\n\t\t\t\t<ul class=\"parallaxbag-popularCategoriesSection-subcategories\">\n\t\t\t\t\t{{#each children}}\n\t\t\t\t\t<li class=\"parallaxbag-popularCategoriesSection-subcategories-item\"><a href=\"{{url}}\">{{name}}</a></li>\n\t\t\t\t\t{{/each}}\n\t\t\t\t</ul>\n\t\t\t\t{{/if}}\n\t\t\t</div>\n\t\t\t{{/each}}\n\t\t</div>\n\t</div>\n</div>\n"
