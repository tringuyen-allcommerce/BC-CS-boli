{
  "components/products/options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small class=\"emthemesModez-requiredMark\">{{lang 'common.required'}}</small>\n        {{/if}}\n\n        {{> components/emthemes-modez/products/field-guide}}\n    </label>\n\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}>{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/emthemes-modez/products/field-guide": "{{#each product_custom_fields}}\n    {{#startsWith (concat (concat \"__\" ../display_name) \"__\") name}}\n        <span class=\"emthemesModez-customField-guide\"><a href=\"{{value}}\" data-emthemesmodez-modal-content>{{#withAfter (split name \"__\") 2}}{{this}}{{/withAfter}}</a></span>\n    {{/startsWith}}\n{{/each}}\n"
}