{
  "components/common/navigation-list-mega2": "<div class=\"navPage-subMenu navPage-subMenu--mega2\" id=\"navPages-{{#if type '===' 'category'}}{{id}}{{else}}page-{{@index}}{{/if}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}} {{> components/emthemes-modez/common/navigation-label}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{#if ../../type '===' 'category'}}{{id}}{{else}}page-{{@../index}}-{{@index}}{{/if}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                        {{> components/emthemes-modez/common/navigation-label}}\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{#if ../../type '===' 'category'}}{{id}}{{else}}page-{{@../index}}-{{@index}}{{/if}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{name}} {{> components/emthemes-modez/common/navigation-label}}</a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">{{name}} {{> components/emthemes-modez/common/navigation-label}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n    <div class=\"navPage-subMenu-banner-list\">\n        <div class=\"navPage-subMenu-banner-item\"><a href=\"https://bolioptics.com/microscopes/biological\"><img src=\"https://cdn7.bigcommerce.com/s-y7qa3t3bno/product_images/uploaded_images/megamenu-bio.jpg\" alt=\"Biological Microscopes\"></a></div>\n        <div class=\"navPage-subMenu-banner-item\"><a href=\"https://bolioptics.com/microscopes/stereo\"><img src=\"https://cdn7.bigcommerce.com/s-y7qa3t3bno/product_images/uploaded_images/megamenu-stereo2.jpg\" alt=\"Stereo Microscopes\"></a></div>\n    </div>\n</div>\n",
  "components/emthemes-modez/common/navigation-label": "{{#if id}}\n{{#if (lang (concat 'emthemesmodez.megamenu.show_item_label_' id)) '==' 'yes'}}\n<span class=\"emthemesModez-navPage-label\" data-emthemesmodez-nav-page-label=\"{{concat 'emthemesmodez.megamenu.item_label_' id}}\">{{lang (concat 'emthemesmodez.megamenu.item_label_' id)}}</span>\n{{/if}}\n{{/if}}\n"
}